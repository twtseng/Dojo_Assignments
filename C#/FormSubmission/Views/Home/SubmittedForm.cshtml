@{
    ViewData["Title"] = "Home Page";
}
@model UserInfo

<div class="text-center">
    <h1 class="display-4">Here is the information you entered</h1>
    <form>
        <div class="form-group">
            <span asp-validation-for="FirstName"></span>
            <label for="FirstName">First Name</label>
            <input type="text"  value="@Model.FirstName" />
        </div>
        <div class="form-group">
            <span asp-validation-for="LastName"></span>
            <label for="LastName">Last Name</label>
            <input type="text"  value="@Model.LastName" />
        </div>
        <div class="form-group">
            <label for="Age">Age</label>
            <input type="text"  value="@Model.Age" />
        </div>
        <div class="form-group">
            <span asp-validation-for="EmailAddress"></span>
            <label for="EmailAddress">EmailAddress</label>
            <input type="text"  value="@Model.EmailAddress" />
        </div>
        <div class="form-group">
            <span asp-validation-for="Password"></span>
            <label for="Password">Password</label>
            <input type="text"  value="@Model.Password" />
        </div>
    </form>
</div>
